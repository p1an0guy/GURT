{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "CanvasSyncResponse.json",
  "title": "CanvasSyncResponse",
  "description": "Response payload for Canvas sync completion",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "synced",
    "coursesUpserted",
    "itemsUpserted",
    "materialsUpserted",
    "materialsMirrored",
    "knowledgeBaseIngestionStarted",
    "knowledgeBaseIngestionJobId",
    "knowledgeBaseIngestionError",
    "failedCourseIds",
    "updatedAt"
  ],
  "properties": {
    "synced": {
      "type": "boolean"
    },
    "coursesUpserted": {
      "type": "integer",
      "minimum": 0
    },
    "itemsUpserted": {
      "type": "integer",
      "minimum": 0
    },
    "materialsUpserted": {
      "type": "integer",
      "minimum": 0
    },
    "materialsMirrored": {
      "type": "integer",
      "minimum": 0
    },
    "knowledgeBaseIngestionStarted": {
      "type": "boolean"
    },
    "knowledgeBaseIngestionJobId": {
      "type": "string"
    },
    "knowledgeBaseIngestionError": {
      "type": "string"
    },
    "failedCourseIds": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "updatedAt": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
    }
  }
}
