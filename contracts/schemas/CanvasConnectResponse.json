{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "CanvasConnectResponse.json",
  "title": "CanvasConnectResponse",
  "description": "Response payload for successful Canvas connection upsert",
  "type": "object",
  "additionalProperties": false,
  "required": ["connected", "updatedAt"],
  "properties": {
    "connected": {
      "type": "boolean"
    },
    "demoUserId": {
      "type": "string",
      "minLength": 1
    },
    "updatedAt": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
    }
  }
}
